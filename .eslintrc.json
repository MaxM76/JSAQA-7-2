{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [ "airbnb-base",
                 "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-debugger": "off",
        "linebreak-style": "off",
        "quotes": [
            "error",      
            "double"      
          ]
    }
}
